{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "The Size object defines the height and width (in pixels) that a publisher accepts",
    "type": "object",
    "properties": {
        "height": {
            "type": "integer",
            "minimum" : 0
        },
        "width": {
            "type": "integer",
            "minimum" : 0
        }
    },
    "required": [ "height", "width" ]
}